{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "start": {
      "persistent": true,
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "build": {
      "outputs": [
        "dist"
      ]
    },
    "check": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "dist"
      ]
    },
    "test": {
      "dependsOn": [
        "build",
        "^test"
      ]
    },
    "lint": {
      "dependsOn": [
        "build",
        "^lint"
      ],
      "outputs": [
        "dist"
      ]
    },
    "lint:autofix": {},
    "format:check": {},
    "format:write": {}
  },
  "globalDependencies": [
    "tsconfig.json"
  ]
}